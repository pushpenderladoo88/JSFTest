<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	
	<managed-bean>
		<managed-bean-name>temperatureConvertor</managed-bean-name>
		<managed-bean-class>de.vogella.jsf.first.model.FileUploadMb</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>keyValuePairData</property-name>
			<value>#{keyValuePairData}</value>
		</managed-property>
		
		
		
	</managed-bean>
	<managed-bean>
		<description>keyValuePairData</description>
		<managed-bean-name>keyValuePairData</managed-bean-name>
		<managed-bean-class>de.vogella.jsf.first.model.KeyValuePairData</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	

</faces-config>